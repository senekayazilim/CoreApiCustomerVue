{
  "name": "onaylarim-core-vue",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:trace": "playwright show-trace",
    "test:e2e:failed": "playwright test --last-failed",
    "test:e2e:clean": "if exist playwright-report rmdir /s /q playwright-report && if exist test-results rmdir /s /q test-results",
    "test:pades": "playwright test tests/e2e/pades.spec.ts --headed --grep P5",
    "test:cades": "playwright test tests/e2e/cades.spec.ts --headed --grep C5 && playwright test tests/e2e/cades.spec.ts --headed --grep \"C6|C7\"",
    "test:all": "npm run test:pades && npm run test:cades"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.14",
    "@heroicons/vue": "^2.0.18",
    "@types/uuid": "^9.0.2",
    "axios": "^1.4.0",
    "uuid": "^9.0.0",
    "vue": "^3.3.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.0.4",
    "vite": "^4.3.5",
    "vue-tsc": "^1.6.4"
  }
}
